@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<div class="text-center">
    <h1 class="display-4">@Model.Title</h1>
</div>
@*<img src="~/images/test.png" />*@

<div id="carouselExampleIndicators" class="carousel slide" data-ride="carousel">
    <ol class="carousel-indicators">
        <li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
        <li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
        <li data-target="#carouselExampleIndicators" data-slide-to="2"></li>
        <li data-target="#carouselExampleIndicators" data-slide-to="3"></li>
        <li data-target="#carouselExampleIndicators" data-slide-to="4"></li>
        <li data-target="#carouselExampleIndicators" data-slide-to="5"></li>
        <li data-target="#carouselExampleIndicators" data-slide-to="6"></li>
        <li data-target="#carouselExampleIndicators" data-slide-to="7"></li>
        <li data-target="#carouselExampleIndicators" data-slide-to="8"></li>
        <li data-target="#carouselExampleIndicators" data-slide-to="9"></li>
        <li data-target="#carouselExampleIndicators" data-slide-to="10"></li>
        <li data-target="#carouselExampleIndicators" data-slide-to="11"></li>

    </ol>
    <div class="carousel-inner">
        <div class="carousel-item active">
            <img class="d-block w-100" src="~/images/mo salah.png" alt="Mohamed Salah">
        </div>
        <div class="carousel-item">
            <img class="d-block w-100" src="~/images/neymar&mbappe.jpg" alt="Neymar Jr. and Kylian Mbappe">
        </div>
        <div class="carousel-item">
            <img class="d-block w-100" src="~/images/messi1.jpg" alt="Lionel Messi">
        </div>
        <div class="carousel-item">
            <img class="d-block w-100" src="~/images/james1.png" alt=" James Rodriguez">
        </div>
        <div class="carousel-item">
            <img class="d-block w-100" src="~/images/son.png" alt="Son Heung-min">
        </div>
        <div class="carousel-item">
            <img class="d-block w-100" src="~/images/cr73.jpg" alt="Cristiano Ronaldo">
        </div>
        <div class="carousel-item">
            <img class="d-block w-100" src="~/images/Philippe.png" alt="Philippe Countinho">
        </div>
        <div class="carousel-item">
            <img class="d-block w-100" src="~/images/marcelo.jpg" alt="Marcelo Vieira">
        </div>
        <div class="carousel-item">
            <img class="d-block w-100" src="~/images/rs1.jpg" alt="Raheem Sterling">
        </div>
        <div class="carousel-item">
            <img class="d-block w-100" src="~/images/paulo.jpg" alt="Paulo Dybala">
        </div>
        <div class="carousel-item">
            <img class="d-block w-100" src="~/images/sergio.jpg" alt="Sergio Ramos">
        </div>
        <div class="carousel-item">
            <img class="d-block w-100" src="~/images/virgil van.jpg" alt="Virgil Van Dijk">
        </div>
    </div>
    <a class="carousel-control-prev" href="#carouselExampleIndicators" role="button" data-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="carousel-control-next" href="#carouselExampleIndicators" role="button" data-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
    </a>
</div>